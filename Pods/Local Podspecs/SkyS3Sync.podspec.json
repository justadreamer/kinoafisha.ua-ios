{
  "name": "SkyS3Sync",
  "version": "1.0.2",
  "summary": "A utility for downsyncing remotely updated versions of local files from S3.  Allows you to remotely modify some application data and make it available to your app without the need to resubmit the app to AppStore or creating a specialized backend and API the app has to talk to",
  "license": {
    "type": "MIT",
    "file": "LICENSE.txt"
  },
  "authors": {
    "Eugene Dorfman": "eugene.dorfman@postindustria.com",
    "Oleg Kovtun": "oleg.kovtun@postindustria.com"
  },
  "source": {
    "git": "git@github.com:justadreamer/SkyS3Sync.git",
    "tag": "1.0.2"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.10"
  },
  "requires_arc": true,
  "homepage": "https://github.com/justadreamer/SkyS3Sync",
  "source_files": [
    "SkyS3Sync/*.{h,m}"
  ],
  "dependencies": {
    "libextobjc": [
      "~> 0.4"
    ],
    "ObjectiveSugar": [
      "~> 1.1"
    ],
    "FileMD5Hash": [
      "~> 2.0"
    ],
    "AFNetworking": [
      "~> 3.0"
    ],
    "Ono": [
      "~> 1.0"
    ]
  }
}
